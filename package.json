{
  "name": "awesomeboggle-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist",
  "scripts": {
    "dev": "nodemon --config 1200ms -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "prestart": "npm run -s build",
    "test": "eslint src && NODE_ENV=test db/setup.sh && NODE_ENV=test jest --runInBand",
    "start": "npm build && node dist"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true
      }
    },
    "env": {
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "globalTeardown": "./__tests__/config.js",
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "eslint-plugin-jest": "^22.1.2",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "passport-headerapikey": "^1.1.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.9.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.6",
    "regenerator-runtime": "^0.13.1",
    "superagent": "^4.0.0",
    "supertest": "^3.3.0"
  }
}
